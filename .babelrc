{
  "presets": [
    [
      "env",
      {
        "targets": {
          "modules": false,
          "node": "current",
          "useBuiltins": true
        }
      }
    ],

    "react",
    "stage-2"
  ],

  "plugins": [
    "transform-es2015-destructuring",
    "transform-object-rest-spread",
    "universal-import"
  ],

  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
